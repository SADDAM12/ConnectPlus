<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.UserProfileController">
   <children>
      <ImageView fx:id="profilePicImgLabel" fitHeight="90.0" fitWidth="105.0" layoutX="29.0" layoutY="27.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="uploadPic" layoutX="165.0" layoutY="72.0" mnemonicParsing="false" onAction="#setImage" text="Upload" />
      <TextField fx:id="nameField" layoutX="44.0" layoutY="188.0" prefHeight="25.0" prefWidth="229.0" />
      <TextField fx:id="address" layoutX="44.0" layoutY="257.0" prefHeight="25.0" prefWidth="229.0" />
      <Button layoutX="174.0" layoutY="330.0" mnemonicParsing="false" onAction="#updateInfo" text="Update Profile" />
      <Label layoutX="44.0" layoutY="159.0" text="Name:" />
      <Label layoutX="44.0" layoutY="231.0" text="Address" />
   </children>
</AnchorPane>
